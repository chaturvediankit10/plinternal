<div class="container">
  <%= link_to "Import Government Sheet", import_government_sheet_import_file_path(@bank), class: "btn btn-primary" %>
  <%= link_to "Import Conforming Fixed Rate",import_conforming_fixed_rate_import_file_path(@bank),  class: "btn btn-primary" %>
  <%= link_to "Import Freddie Fixed Rate", import_freddie_fixed_rate_import_file_path(@bank),class: "btn btn-primary" %>
  <%= link_to "Import Home Possible", home_possible_import_file_path(@bank),class: "btn btn-primary" %>
  <%= link_to "Import Conforming ARMs", conforming_arms_import_file_path(@bank),class: "btn btn-primary" %>

  <h2>Banks Details</h2>
  <table class="table table-dark table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Phone</th>
        <th>Address</th>
        <th>State Code</th>
        <th>Zip</th>
        <th>Programs</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% Bank.all.each do |bank|%>
          <td><%= bank.name %></td>
          <td><%= bank.phone %></td>
          <td><%= bank.address1 %></td>
          <td><%= bank.state_code %></td>
          <td><%= bank.zip %></td>
          <td><%= link_to "Programs", programs_import_file_path(bank) %></td>
        <%end%>
      </tr>
    </tbody>
  </table>
</div>
