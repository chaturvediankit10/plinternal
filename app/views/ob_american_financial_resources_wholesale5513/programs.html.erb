<div class="container">
  <div class="row col-md-6 col-md-offset-2 custyle">
  <table class="table table-striped custab">
  <thead>
  <h2><%= @sheet_obj.name.capitalize + " Programs List" %></h2>
  <tr>
    <th>No.</th>
    <th>Name</th>
    <th class="text-center">Action</th>
  </tr>
  </thead>
  <% if @programs.present? %>
    <% @programs.each_with_index do |program, index|%>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= program.program_name %></td>
        <td class="text-center"><a class='btn btn-info btn-xs' href=<%= "/ob_american_financial_resources_wholesale5513/"+ program.id.to_s + "/single_program" %>><span class="glyphicon glyphicon-edit"></span> View</a></td>
      </tr>
    <%end%>
  <% end %>
  </table>
  </div>
</div>
